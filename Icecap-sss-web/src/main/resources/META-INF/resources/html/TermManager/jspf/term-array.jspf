				<aui:col md="2">
					<liferay-ui:message key="term-array-type"/>
				</aui:col>
				<aui:col md="2">
					<aui:input type="radio" name="arrayUnitType" label="term-array-type-string" value="String"></aui:input>
				</aui:col>
				<aui:col md="2">
					<aui:input type="radio" name="arrayUnitType" label="term-array-type-numeric" value="Numeric"></aui:input>
				</aui:col>
			</aui:row>

			<aui:container>
				<aui:row>			
