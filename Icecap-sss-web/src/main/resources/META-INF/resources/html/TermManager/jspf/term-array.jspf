						<span>&#8491;</span>
					</aui:col>
				</aui:row>
				<aui:row>
					<aui:col md="2">
						<liferay-ui:message key="term-array-element-uncertainty"/>
					</aui:col>
					<aui:col md="2">
						<aui:input type="checkbox" name="elementUncertainty" label="term-array-element-uncertainty-approve"></aui:input>
					</aui:col>
				</aui:row>
			</aui:container>
		</aui:fieldset>
